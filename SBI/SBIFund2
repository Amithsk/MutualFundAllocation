import xlrd
workbook =xlrd.open_workbook('SBIFund.xls')
sheetNo = workbook.nsheets
rowCount=0
colsCount=3
rowsCount=800
fundList=[[0 for x in range(colsCount)]for y in range(rowsCount)]
for list in range(1,sheetNo):
  for rowIt in range(0,workbook.sheet_by_index(list).nrows):
    if workbook.sheet_by_index(list).cell_value(rowIt,5)== "Banks":
       fundList[rowCount][2]=str(workbook.sheet_by_index(list).cell_value(rowIt,2))
       fundList[rowCount][1]=str(workbook.sheet_by_index(list).cell_value(rowIt,8))
       fundList[rowCount][0]=str(workbook.sheet_by_index(list).name)
       rowCount=rowCount+1

#for lp in range (rowCount):
#  for rowlt in range(0,workbook.sheet_by_index(0).nrows):
#    print(fundList[rowCount][0],workbook.sheet_by_index(0).cell_value(rowlt,1))
#    if workbook.sheet_by_index(0).cell_value(rowlt,1)==fundList[rowCount][0]:
#      fundList[rowCount][0]=str(workbook.sheet_by_index(0).cell_value(rowlt,2))

for lp in range(rowCount):
  print(fundList[lp][0],fundList[lp][1],fundList[lp][2])
